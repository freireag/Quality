<h1>Editing admin_file</h1>

<% form_for(@admin_file) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :permission %><br />
    <%= f.text_field :permission %>
  </p>
  <p>
    <%= f.label :course %><br />
    <%= f.text_field :course %>
  </p>
  <p>
    <%= f.label :user %><br />
    <%= f.text_field :user %>
  </p>
  <p>
    <%= f.label :attachment_file_name %><br />
    <%= f.text_field :attachment_file_name %>
  </p>
  <p>
    <%= f.label :attachment_content_type %><br />
    <%= f.text_field :attachment_content_type %>
  </p>
  <p>
    <%= f.label :attachment_file_size %><br />
    <%= f.text_field :attachment_file_size %>
  </p>
  <p>
    <%= f.label :attachment_updated_at %><br />
    <%= f.datetime_select :attachment_updated_at %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @admin_file %> |
<%= link_to 'Back', admin_files_path %>